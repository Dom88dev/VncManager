<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <Label fx:id="noticeText" alignment="CENTER" prefHeight="64.0" prefWidth="320.0" text="안내문구" textFill="#9161c6" BorderPane.alignment="CENTER">
         <font>
            <Font size="11.0" />
         </font></Label>
   </center>
   <top>
      <ImageView fitHeight="97.0" fitWidth="333.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@images/notice.png" />
         </image></ImageView>
   </top>
   <effect>
      <DropShadow color="#9161c6" />
   </effect>
</BorderPane>
